{
  "name": "irc-gateway",
  "version": "2.0.0",
  "description": "A chat system for online games, which allows mirroring into IRC",
  "keywords": [
    "irc",
    "chat",
    "chatsystem",
    "bot",
    "game",
    "gateway",
    "mirror",
    "bridge",
    "proxy"
  ],
  "author": {
    "name": "Atanamo",
    "url": "https://github.com/atanamo"
  },
  "homepage": "https://github.com/atanamo/irc-gateway",
  "repository": {
    "type": "git",
    "url": "https://github.com/atanamo/irc-gateway.git"
  },
  "license": "BSD-3-Clause",
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "main": "dist/server/index.js",
  "scripts": {
    "prepublish": "npm run build & npm run bundle-client",
    "build": "./node_modules/.bin/cake build",
    "watch": "./node_modules/.bin/cake watch",
    "bundle-client": "./node_modules/.bin/cake bundle-client",
    "demo": "node ./sample/server.js"
  },
  "dependencies": {
    "q": "~1.5.1",
    "express": "~4.16.2",
    "socket.io": "~1.7.4",
    "irc": "~0.5.2",
    "mysql": "~2.15.0"
  },
  "devDependencies": {
    "coffeescript": "1.12.7"
  },
  "bundleDependencies": []
}
